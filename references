UPDATE bse.bse_200
set DATE =  str_to_date(Date,"%d-%M-%Y")
WHERE `Load.Type` = 'DUMP'

UPDATE bse.bse_200
set DATE =  str_to_date(Date,"%d-%b-%Y")
WHERE `Load.Type` = 'DAILY'

INCREASE PATTERN:
1.  US = 2*B
2.  B = 3*US

